{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "strict": 0,
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "function-paren-newline": "off",
    "react/no-children-prop": 0,
    "import/no-named-as-default": 0,
    "no-nested-ternary": 0,
    "no-unused-expressions": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "prefer-const": 1,
    "complexity": [
      1,
      5
    ],
    "react/prefer-es6-class": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-curly-spacing": [
      2,
      "always"
    ],
    "react/jsx-indent": [
      2
    ]
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}